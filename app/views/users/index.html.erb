<div class="jumbotron center">
    <h1>Listing all Users</h1>
    <p>
    <%= link_to "Create a User", new_user_path, class: "btn btn-danger btn-lg btn-xlarge" %>
    </p>
    <table class="table">
    <tr>
    <th>Name</th>
    <th>Email</th>
    <th></th>
    <th></th>
    <th></th>
    </tr>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
      <td><%= link_to "Edit", edit_user_path(user) %></td>
      <td><%= link_to "Show", user_path(user) %></td>
      <td><%= link_to "Delete", user_path(user), method: :delete, data: {confirm: "Are you sure?"} %> </td>
    </tr>
    <% end %>
  </table>
</div>